{
  "name": "claude-paste-image-ext",
  "displayName": "Claude Paste Image Extension",
  "description": "Paste clipboard images and insert paths for Claude Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:claude-paste-image.pasteImage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claude-paste-image.pasteImage",
        "title": "Paste Image for Claude Code"
      }
    ],
    "keybindings": [
      {
        "command": "claude-paste-image.pasteImage",
        "key": "ctrl+shift+v",
        "mac": "cmd+shift+v",
        "when": "terminalFocus"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "clipboardy": "^3.0.0"
  }
}